
{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["esnext", "WebWorker"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "outDir": "./dist"
  }
}